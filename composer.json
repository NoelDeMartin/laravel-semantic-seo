{
    "name": "noeldemartin/laravel-semantic-seo",
    "description": "Provide utilities to add Semantic SEO to Laravel Applications.",
    "keywords": ["laravel", "seo", "semantic-html", "semantic-seo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Noel De Martin",
            "email": "noeldemartin@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "php": ">=8.2.0",
        "illuminate/support": ">=11.0.0"
    },
    "require-dev": {
        "illuminate/container": "^11.38",
        "illuminate/routing": "^11.38",
        "fakerphp/faker": "^1.24",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.5",
        "tightenco/duster": "^3.1"
    },
    "autoload": {
        "psr-4" : {
            "NoelDeMartin\\SemanticSEO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testing\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NoelDeMartin\\SemanticSEO\\SemanticSEOServiceProvider"
            ],
            "aliases": {
                "SemanticSEO": "NoelDeMartin\\SemanticSEO\\Support\\Facades\\SemanticSEO"
            }
        }
    }
}
