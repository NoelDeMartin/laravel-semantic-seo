{
    "name": "noeldemartin/laravel-semantic-seo",
    "description": "Provide utilites to add Semantic SEO to Laravel Applications.",
    "keywords": ["laravel", "seo"],
    "license": "MIT",
    "authors": [
        {
            "name": "Noel De Martin",
            "email": "noeldemartin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "^1.7"
    },
    "autoload": {
        "psr-4" : {
            "NoelDeMartin\\SemanticSEO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Testing\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NoelDeMartin\\SemanticSEO\\SemanticSEOServiceProvider"
            ]
        }
    }
}